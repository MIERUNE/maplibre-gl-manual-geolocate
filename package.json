{
  "name": "maplibre-gl-mock-geolocate",
  "version": "0.1.0",
  "description": "A MapLibre GL JS control that displays a user position marker at specified coordinates without requiring the browser's geolocation API",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/maplibre-gl-mock-geolocate.umd.cjs",
  "module": "./dist/maplibre-gl-mock-geolocate.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/maplibre-gl-mock-geolocate.js",
      "require": "./dist/maplibre-gl-mock-geolocate.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "maplibre",
    "geolocate",
    "mock",
    "typescript",
    "maps"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/maplibre-gl-mock-geolocate.git"
  },
  "peerDependencies": {
    "maplibre-gl": "^3.0.0 || ^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "maplibre-gl": "^5.7.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.2"
  }
}
