{
  "name": "maplibre-gl-mock-geolocate",
  "version": "0.1.0",
  "description": "A MapLibre GL JS control that displays a user position marker at specified coordinates without requiring the browser's geolocation API",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MIERUNE/maplibre-gl-mock-geolocate.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/maplibre-gl-mock-geolocate.umd.cjs",
  "module": "./dist/maplibre-gl-mock-geolocate.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/maplibre-gl-mock-geolocate.js",
      "require": "./dist/maplibre-gl-mock-geolocate.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build --config vite.config.lib.ts",
    "build:demo": "vite build --config vite.config.demo.ts",
    "preview": "vite preview",
    "preview:demo": "vite preview --config vite.config.demo.ts"
  },
  "peerDependencies": {
    "maplibre-gl": ">=5.0.0"
  },
  "devDependencies": {
    "maplibre-gl": "^5.7.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4"
  }
}
